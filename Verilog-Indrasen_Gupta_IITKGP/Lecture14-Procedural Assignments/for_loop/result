#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x644f2159cd90 .scope module, "count" "count" 2 1;
 .timescale 0 0;
v0x644f2159e350_0 .var/i "count", 31 0;
v0x644f2159e7b0_0 .var "data", 100 1;
v0x644f2159eba0_0 .var/i "i", 31 0;
    .scope S_0x644f2159cd90;
T_0 ;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x644f2159e350_0, 0, 32;
T_0.0 ;
    %load/vec4 v0x644f2159e350_0;
    %cmpi/s 10, 0, 32;
    %flag_or 5, 4;
    %jmp/0xz T_0.1, 5;
    %vpi_call 2 8 "$display", "My count : %d ", v0x644f2159e350_0 {0 0 0};
    %load/vec4 v0x644f2159e350_0;
    %addi 1, 0, 32;
    %store/vec4 v0x644f2159e350_0, 0, 32;
    %jmp T_0.0;
T_0.1 ;
    %end;
    .thread T_0;
    .scope S_0x644f2159cd90;
T_1 ;
    %pushi/vec4 1, 0, 32;
    %store/vec4 v0x644f2159eba0_0, 0, 32;
T_1.0 ;
    %load/vec4 v0x644f2159eba0_0;
    %cmpi/s 100, 0, 32;
    %flag_or 5, 4;
    %jmp/0xz T_1.1, 5;
    %pushi/vec4 0, 0, 1;
    %load/vec4 v0x644f2159eba0_0;
    %pad/s 34;
    %subi 1, 0, 34;
    %ix/vec4/s 4;
    %store/vec4 v0x644f2159e7b0_0, 4, 1;
    %load/vec4 v0x644f2159eba0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x644f2159eba0_0, 0, 32;
    %jmp T_1.0;
T_1.1 ;
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 3;
