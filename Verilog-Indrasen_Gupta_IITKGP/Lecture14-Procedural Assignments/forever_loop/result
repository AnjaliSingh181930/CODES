 .timescale 0 0;
v0x62f0952f0350_0 .var "clock", 0 0;
    .scope S_0x62f0953137a0;
T_0 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x62f0952f0350_0, 0, 1;
T_0.0 ;
    %delay 5, 0;
    %load/vec4 v0x62f0952f0350_0;
    %inv;
    %store/vec4 v0x62f0952f0350_0, 0, 1;
    %jmp T_0.0;
    %end;
    .thread T_0;
    .scope S_0x62f0953137a0;
