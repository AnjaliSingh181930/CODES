#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x57c052364470 .scope module, "testbench" "testbench" 2 1;
 .timescale 0 0;
S_0x57c052364600 .scope module, "uut" "count" 2 6, 3 1 0, S_0x57c052364470;
 .timescale 0 0;
v0x57c05232c350_0 .var/i "count", 31 0;
    .scope S_0x57c052364600;
T_0 ;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x57c05232c350_0, 0, 32;
T_0.0 ;
    %load/vec4 v0x57c05232c350_0;
    %cmpi/s 10, 0, 32;
    %flag_or 5, 4;
    %jmp/0xz T_0.1, 5;
    %vpi_call 3 9 "$display", "My count : %d ", v0x57c05232c350_0 {0 0 0};
    %load/vec4 v0x57c05232c350_0;
    %addi 1, 0, 32;
    %store/vec4 v0x57c05232c350_0, 0, 32;
    %jmp T_0.0;
T_0.1 ;
    %end;
    .thread T_0;
    .scope S_0x57c052364470;
T_1 ;
    %vpi_call 2 11 "$display", "Starting simulation..." {0 0 0};
    %vpi_call 2 14 "$dumpfile", "count.vcd" {0 0 0};
    %vpi_call 2 15 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x57c052364470 {0 0 0};
    %delay 100, 0;
    %vpi_call 2 21 "$finish" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "test.v";
    "count.v";
