#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5c5543075d90 .scope module, "non_blocking_assgn" "non_blocking_assgn" 2 1;
 .timescale 0 0;
v0x5c55430777b0_0 .var/i "a", 31 0;
v0x5c5543077ba0_0 .var/i "b", 31 0;
v0x5c55430c03a0_0 .var/i "c", 31 0;
v0x5c55430c0460_0 .var/i "d", 31 0;
E_0x5c5543076470 .event edge, v0x5c5543077ba0_0, v0x5c55430c03a0_0, v0x5c55430777b0_0, v0x5c55430c0460_0;
    .scope S_0x5c5543075d90;
T_0 ;
    %wait E_0x5c5543076470;
    %pushi/vec4 4, 0, 32;
T_0.0 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_0.1, 5;
    %jmp/1 T_0.1, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %load/vec4 v0x5c5543077ba0_0;
    %load/vec4 v0x5c55430c03a0_0;
    %add;
    %assign/vec4 v0x5c55430777b0_0, 5;
    %load/vec4 v0x5c55430777b0_0;
    %subi 3, 0, 32;
    %assign/vec4 v0x5c55430c0460_0, 5;
    %load/vec4 v0x5c55430c0460_0;
    %addi 10, 0, 32;
    %assign/vec4 v0x5c5543077ba0_0, 5;
    %jmp T_0.0;
T_0.1 ;
    %pop/vec4 1;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x5c5543075d90;
T_1 ;
    %vpi_call 2 14 "$monitor", $time, "  a = %4d, b = %4d, c = %4d, d = %4d", v0x5c55430777b0_0, v0x5c5543077ba0_0, v0x5c55430c03a0_0, v0x5c55430c0460_0 {0 0 0};
    %pushi/vec4 30, 0, 32;
    %store/vec4 v0x5c55430777b0_0, 0, 32;
    %pushi/vec4 20, 0, 32;
    %store/vec4 v0x5c5543077ba0_0, 0, 32;
    %pushi/vec4 15, 0, 32;
    %store/vec4 v0x5c55430c03a0_0, 0, 32;
    %pushi/vec4 5, 0, 32;
    %store/vec4 v0x5c55430c0460_0, 0, 32;
    %delay 200, 0;
    %vpi_call 2 19 "$finish" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
