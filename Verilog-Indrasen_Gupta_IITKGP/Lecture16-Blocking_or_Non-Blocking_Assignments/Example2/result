:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x61edd1c7fd90 .scope module, "blocking_assgn" "blocking_assgn" 2 1;
 .timescale 0 0;
v0x61edd1c817b0_0 .var/i "a", 31 0;
v0x61edd1c81ba0_0 .var/i "b", 31 0;
v0x61edd1cca730_0 .var/i "c", 31 0;
v0x61edd1cca7f0_0 .var/i "d", 31 0;
E_0x61edd1c80470 .event edge, v0x61edd1c81ba0_0, v0x61edd1cca730_0, v0x61edd1c817b0_0, v0x61edd1cca7f0_0;
    .scope S_0x61edd1c7fd90;
T_0 ;
    %wait E_0x61edd1c80470;
    %pushi/vec4 4, 0, 32;
T_0.0 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_0.1, 5;
    %jmp/1 T_0.1, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %delay 5, 0;
    %load/vec4 v0x61edd1c81ba0_0;
    %load/vec4 v0x61edd1cca730_0;
    %add;
    %store/vec4 v0x61edd1c817b0_0, 0, 32;
    %delay 5, 0;
    %load/vec4 v0x61edd1c817b0_0;
    %subi 3, 0, 32;
    %store/vec4 v0x61edd1cca7f0_0, 0, 32;
    %delay 5, 0;
    %load/vec4 v0x61edd1cca7f0_0;
    %addi 10, 0, 32;
    %store/vec4 v0x61edd1c81ba0_0, 0, 32;
    %delay 5, 0;
    %load/vec4 v0x61edd1cca730_0;
    %addi 1, 0, 32;
    %store/vec4 v0x61edd1cca730_0, 0, 32;
    %jmp T_0.0;
T_0.1 ;
    %pop/vec4 1;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x61edd1c7fd90;
T_1 ;
    %vpi_call 2 15 "$monitor", $time, "  a = %4d, b = %4d, c = %4d, d = %4d", v0x61edd1c817b0_0, v0x61edd1c81ba0_0, v0x61edd1cca730_0, v0x61edd1cca7f0_0 {0 0 0};
    %pushi/vec4 30, 0, 32;
    %store/vec4 v0x61edd1c817b0_0, 0, 32;
    %pushi/vec4 20, 0, 32;
    %store/vec4 v0x61edd1c81ba0_0, 0, 32;
    %pushi/vec4 15, 0, 32;
    %store/vec4 v0x61edd1cca730_0, 0, 32;
    %pushi/vec4 5, 0, 32;
    %store/vec4 v0x61edd1cca7f0_0, 0, 32;
    %delay 200, 0;
    %vpi_call 2 20 "$finish" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
