T_1 ;
    %wait E_0x5823f9300e70;
    %load/vec4 v0x5823f9301ba0_0;
    %load/vec4 v0x5823f934ab20_0;
    %add;
    %assign/vec4 v0x5823f93017b0_0, 0;
    %load/vec4 v0x5823f93017b0_0;
    %subi 3, 0, 32;
    %assign/vec4 v0x5823f934aca0_0, 0;
    %load/vec4 v0x5823f934aca0_0;
    %addi 10, 0, 32;
    %assign/vec4 v0x5823f934ab20_0, 0;
    %load/vec4 v0x5823f934ab20_0;
    %addi 1, 0, 32;
    %assign/vec4 v0x5823f934ab20_0, 0;
    %jmp T_1;
    .thread T_1;
    .scope S_0x5823f92ffd90;
T_2 ;
    %vpi_call 2 16 "$monitor", $time, "  a = %4d, b = %4d, c = %4d, d = %4d", v0x5823f93017b0_0, v0x5823f9301ba0_0, v0x5823f934ab20_0, v0x5823f934aca0_0 {0 0 0};
    %pushi/vec4 30, 0, 32;
    %store/vec4 v0x5823f93017b0_0, 0, 32;
    %pushi/vec4 20, 0, 32;
    %store/vec4 v0x5823f9301ba0_0, 0, 32;
    %pushi/vec4 15, 0, 32;
    %store/vec4 v0x5823f934ab20_0, 0, 32;
    %pushi/vec4 5, 0, 32;
    %store/vec4 v0x5823f934aca0_0, 0, 32;
T_2.0 ;
    %delay 5, 0;
    %load/vec4 v0x5823f934abe0_0;
    %inv;
    %store/vec4 v0x5823f934abe0_0, 0, 1;
    %jmp T_2.0;
    %end;
