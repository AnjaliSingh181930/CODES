S_0x62213b831d90 .scope module, "non_blocking_assgn" "non_blocking_assgn" 2 1;
 .timescale 0 0;
v0x62213b8337b0_0 .var/i "a", 31 0;
v0x62213b833ba0_0 .var/i "b", 31 0;
v0x62213b87c8b0_0 .var/i "c", 31 0;
v0x62213b87c970_0 .var "clk", 0 0;
v0x62213b87ca30_0 .var/i "d", 31 0;
E_0x62213b832e70 .event posedge, v0x62213b87c970_0;
    .scope S_0x62213b831d90;
T_0 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x62213b87c970_0, 0, 1;
    %end;
    .thread T_0;
    .scope S_0x62213b831d90;
T_1 ;
    %delay 5, 0;
    %load/vec4 v0x62213b87c970_0;
    %inv;
    %store/vec4 v0x62213b87c970_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x62213b831d90;
T_2 ;
    %wait E_0x62213b832e70;
    %load/vec4 v0x62213b833ba0_0;
    %load/vec4 v0x62213b87c8b0_0;
    %and;
    %assign/vec4 v0x62213b8337b0_0, 0;
    %load/vec4 v0x62213b8337b0_0;
    %load/vec4 v0x62213b87ca30_0;
    %xor;
    %assign/vec4 v0x62213b87ca30_0, 0;
    %load/vec4 v0x62213b8337b0_0;
    %load/vec4 v0x62213b833ba0_0;
    %or;
    %assign/vec4 v0x62213b87c8b0_0, 0;
    %jmp T_2;
    .thread T_2;
    .scope S_0x62213b831d90;
T_3 ;
    %vpi_call 2 20 "$monitor", $time, "  a = %4d, b = %4d, c = %4d, d = %4d", v0x62213b8337b0_0, v0x62213b833ba0_0, v0x62213b87c8b0_0, v0x62213b87ca30_0 {0 0 0};
    %pushi/vec4 30, 0, 32;
    %store/vec4 v0x62213b8337b0_0, 0, 32;
    %pushi/vec4 20, 0, 32;
    %store/vec4 v0x62213b833ba0_0, 0, 32;
    %pushi/vec4 15, 0, 32;
    %store/vec4 v0x62213b87c8b0_0, 0, 32;
    %pushi/vec4 5, 0, 32;
    %store/vec4 v0x62213b87ca30_0, 0, 32;
    %delay 200, 0;
    %vpi_call 2 25 "$finish" {0 0 0};
    %end;
    .thread T_3;
