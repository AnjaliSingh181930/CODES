#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5957b62aa740 .scope module, "testbench" "testbench" 2 1;
 .timescale 0 0;
v0x5957b62bdbe0_0 .var "clear", 0 0;
v0x5957b62bdcb0_0 .var "clock", 0 0;
v0x5957b62bdd80_0 .net "count", 7 0, v0x5957b62bda70_0;  1 drivers
S_0x5957b62aa8d0 .scope module, "DUT" "counter" 2 7, 3 1 0, S_0x5957b62aa740;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clear";
    .port_info 1 /INPUT 1 "clock";
    .port_info 2 /OUTPUT 8 "count";
P_0x5957b62aaab0 .param/l "N" 0 3 2, +C4<00000000000000000000000000000111>;
v0x5957b6272ba0_0 .net "clear", 0 0, v0x5957b62bdbe0_0;  1 drivers
v0x5957b62bd9b0_0 .net "clock", 0 0, v0x5957b62bdcb0_0;  1 drivers
v0x5957b62bda70_0 .var "count", 0 7;
E_0x5957b6271600 .event negedge, v0x5957b62bd9b0_0;
    .scope S_0x5957b62aa8d0;
T_0 ;
    %wait E_0x5957b6271600;
    %load/vec4 v0x5957b6272ba0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 8;
    %assign/vec4 v0x5957b62bda70_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x5957b62bda70_0;
    %addi 1, 0, 8;
    %assign/vec4 v0x5957b62bda70_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x5957b62aa740;
T_1 ;
    %vpi_call 2 11 "$dumpfile", "counter.vcd" {0 0 0};
    %vpi_call 2 12 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5957b62aa740 {0 0 0};
    %vpi_call 2 15 "$monitor", $time, " clear = %b, clock = %b, count = %b", v0x5957b62bdbe0_0, v0x5957b62bdcb0_0, v0x5957b62bdd80_0 {0 0 0};
    %delay 5, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5957b62bdbe0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5957b62bdcb0_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5957b62bdbe0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5957b62bdcb0_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5957b62bdbe0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5957b62bdcb0_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5957b62bdbe0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5957b62bdcb0_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5957b62bdbe0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5957b62bdcb0_0, 0, 1;
    %vpi_call 2 25 "$finish" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
