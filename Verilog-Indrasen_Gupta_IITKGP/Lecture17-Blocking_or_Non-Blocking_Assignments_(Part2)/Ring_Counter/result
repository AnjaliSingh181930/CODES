#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x576729248b40 .scope module, "testbench" "testbench" 2 1;
 .timescale 0 0;
v0x57672925b080_0 .var "clk", 0 0;
v0x57672925b150_0 .net "count", 7 0, v0x576729210ba0_0;  1 drivers
v0x57672925b220_0 .var "init", 0 0;
S_0x576729248cd0 .scope module, "uut" "ring_counter" 2 8, 3 1 0, S_0x576729248b40;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "init";
    .port_info 2 /OUTPUT 8 "count";
v0x5767292107b0_0 .net "clk", 0 0, v0x57672925b080_0;  1 drivers
v0x576729210ba0_0 .var "count", 7 0;
v0x57672925af60_0 .net "init", 0 0, v0x57672925b220_0;  1 drivers
E_0x576729247a20 .event posedge, v0x5767292107b0_0;
    .scope S_0x576729248cd0;
T_0 ;
    %wait E_0x576729247a20;
    %load/vec4 v0x57672925af60_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 128, 0, 8;
    %assign/vec4 v0x576729210ba0_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x576729210ba0_0;
    %ix/load 4, 1, 0;
    %flag_set/imm 4, 0;
    %shiftl 4;
    %assign/vec4 v0x576729210ba0_0, 0;
    %load/vec4 v0x576729210ba0_0;
    %parti/s 1, 7, 4;
    %ix/load 4, 0, 0;
    %ix/load 5, 0, 0;
    %flag_set/imm 4, 0;
    %assign/vec4/off/d v0x576729210ba0_0, 4, 5;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x576729248b40;
T_1 ;
    %delay 2, 0;
    %load/vec4 v0x57672925b080_0;
    %inv;
    %store/vec4 v0x57672925b080_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x576729248b40;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x57672925b080_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x57672925b220_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x57672925b220_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x57672925b220_0, 0, 1;
    %delay 100, 0;
    %delay 50, 0;
    %vpi_call 2 27 "$finish" {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x576729248b40;
T_3 ;
    %vpi_call 2 32 "$monitor", "At time %t: count = %b", $time, v0x57672925b150_0 {0 0 0};
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "test.v";
    "counter.v";
